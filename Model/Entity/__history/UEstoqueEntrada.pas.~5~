unit UEstoqueEntrada;

interface

type
  TEstoqueEntrada = class
    private
      FCodigo        : Integer;
      FCodProduto    : Integer;
      FCodLocal      : Integer;
      FLote          : String;
      FDataFabricacao: TDate;
      FDataVencimento: TDate;
      FQuantidade    : Double;
      FDataHora      : TDateTime;
    public
      property Codigo        : Integer;
      property CodProduto    : Integer;
      property CodLocal      : Integer;
      property Lote          : String;
      property DataFabricacao: TDate;
      property DataVencimento: TDate;
      property Quantidade    : Double;
      property DataHora      : TDateTime;
  end;

implementation

end.
